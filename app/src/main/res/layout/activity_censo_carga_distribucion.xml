<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorLightPrimary"
    tools:context=".CensoCargaDistribucion">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.google.android.material.appbar.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/AppTheme.AppBarOverlay">

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="4dp"
                android:layout_marginRight="2dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <EditText
                    android:id="@+id/txt_buscar_elemento"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:autofillHints="@string/buscar_elemento"
                    android:ems="10"
                    android:hint="@string/buscar_elemento"
                    android:inputType="number"
                    android:maxLength="7"
                    tools:text="@string/buscar_elemento" />

                <ImageButton
                    android:id="@+id/btn_buscar_elemento"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:background="@color/colorPrimary"
                    android:contentDescription="@string/buscar_elemento"
                    android:src="@drawable/icon_search"
                    android:tint="@color/colorIconText" />

                <ImageButton
                    android:id="@+id/btn_limpiar"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:background="@color/colorPrimary"
                    android:src="@drawable/icon_cancel"
                    android:tint="@color/colorIconText" />

            </LinearLayout>

        </com.google.android.material.appbar.AppBarLayout>

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginBottom="4dp"
                    android:orientation="vertical">

                    <EditText
                        android:id="@+id/txt_elemento_no"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:inputType="number"
                        android:maxLength="6"
                        android:visibility="invisible" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        app:cardBackgroundColor="@color/backgroundApp"
                        app:cardCornerRadius="5dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="3dp"
                            android:orientation="vertical">

                            <Switch
                                android:id="@+id/sw_numero_luminaria_visible"
                                android:layout_width="match_parent"
                                android:layout_height="60dp"
                                android:layout_marginTop="2dp"
                                android:layout_marginBottom="2dp"
                                android:checked="true"
                                android:text="¿El Número de la Luminaria es  Visible?" />

                            <View
                                android:id="@+id/divider4"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="?android:attr/listDivider" />

                            <Switch
                                android:id="@+id/sw_tiene_luminaria"
                                android:layout_width="match_parent"
                                android:layout_height="60dp"
                                android:checked="true"
                                android:text="¿Posee Luminaria?" />

                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        app:cardBackgroundColor="@color/backgroundApp"
                        app:cardCornerRadius="5dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="3dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textViewCensoAsignado"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Censo Asignado *" />

                            <Spinner
                                android:id="@+id/slt_censo_asignado"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:drawable/btn_dropdown" />
                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <!--Ref Elemento-->
                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        app:cardBackgroundColor="@color/backgroundApp"
                        app:cardCornerRadius="5dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="3dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Tipologia" />

                            <Spinner
                                android:id="@+id/slt_tipologia"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:drawable/btn_dropdown" />

                            <TextView
                                android:id="@+id/textView3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Mobiliario" />

                            <Spinner
                                android:id="@+id/slt_mobiliario"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:drawable/btn_dropdown" />

                            <TextView
                                android:id="@+id/textView4"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Referencia" />

                            <Spinner
                                android:id="@+id/slt_referencia"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:drawable/btn_dropdown" />

                            <TextView
                                android:id="@+id/textView24"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Funcionamiento del mobiliario" />

                            <Spinner
                                android:id="@+id/slt_estado_mobiliario"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:drawable/btn_dropdown" />


                            <Switch
                                android:id="@+id/sw_mobiliario_en_buenas_condiciones"
                                android:layout_width="match_parent"
                                android:layout_height="60dp"
                                android:checked="true"
                                android:text="¿Mobiliario en buenas condiciones físicas?" />


                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>
                    <!--Fin Referencia-->

                    <!--Ubicacion-->
                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        app:cardBackgroundColor="@color/backgroundApp"
                        app:cardCornerRadius="5dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="5dp"
                            android:orientation="vertical">


                            <TextView
                                android:id="@+id/textView5"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Barrio *" />

                            <Spinner
                                android:id="@+id/slt_barrio"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:drawable/btn_dropdown" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/textView25"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Zona" />

                                    <RadioGroup
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent">

                                        <RadioButton
                                            android:id="@+id/rd_urbano"
                                            android:layout_width="match_parent"
                                            android:layout_height="40dp"
                                            android:checked="true"
                                            android:text="Urbana" />

                                        <RadioButton
                                            android:id="@+id/rd_rutal"
                                            android:layout_width="match_parent"
                                            android:layout_height="40dp"
                                            android:text="Rural" />

                                    </RadioGroup>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/textView26"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Sector" />

                                    <RadioGroup
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent">

                                        <RadioButton
                                            android:id="@+id/rd_normal"
                                            android:layout_width="match_parent"
                                            android:layout_height="40dp"
                                            android:checked="true"
                                            android:text="Normal" />

                                        <RadioButton
                                            android:id="@+id/rd_subnormal"
                                            android:layout_width="match_parent"
                                            android:layout_height="40dp"
                                            android:text="SubNormal" />
                                    </RadioGroup>

                                </LinearLayout>
                            </LinearLayout>

                            <TextView
                                android:id="@+id/textView23"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Dirección *" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <EditText
                                    android:id="@+id/txt_direccion"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:background="@android:drawable/edit_text"
                                    android:ems="10"
                                    android:inputType="textPersonName" />

                                <ImageButton
                                    android:id="@+id/btn_editar_direccion"
                                    android:layout_width="90dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:background="@android:drawable/btn_default_small"
                                    android:tint="@android:color/holo_green_dark"
                                    app:srcCompat="@android:drawable/ic_menu_edit" />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/view_latitud"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="Lat:" />

                                <TextView
                                    android:id="@+id/gps_latitud"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:text=""
                                    android:textColor="@color/design_default_color_primary_dark" />

                                <TextView
                                    android:id="@+id/view_longitud"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="Lon:" />

                                <TextView
                                    android:id="@+id/gps_longitud"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:text=""
                                    android:textColor="@color/design_default_color_primary_dark" />

                            </LinearLayout>

                            <!--coordenadas-->
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/view_altitud"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="Alt:" />

                                <TextView
                                    android:id="@+id/gps_altitud"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:text=""
                                    android:textColor="@color/design_default_color_primary_dark" />

                                <TextView
                                    android:id="@+id/view_precision"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="Pre:" />

                                <TextView
                                    android:id="@+id/gps_precision"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:text=""
                                    android:textColor="@color/design_default_color_primary_dark" />

                                <TextView
                                    android:id="@+id/view_velocidad"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="Vel:" />

                                <TextView
                                    android:id="@+id/gps_velocidad"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:text=""
                                    android:textColor="@color/design_default_color_primary_dark" />
                            </LinearLayout>
                            <!--fin coordenadas-->

                            <!--direccion-->
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/view_direccion"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="Dir:" />

                                <TextView
                                    android:id="@+id/gps_direccion"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="4"
                                    android:text=""
                                    android:textColor="@color/design_default_color_primary_dark" />

                            </LinearLayout>
                            <!--fin direccion-->

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="2dp"
                                    android:layout_weight="1"
                                    android:orientation="vertical">

                                    <EditText
                                        android:id="@+id/txt_latitud"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:ems="10"
                                        android:hint="Latitud"
                                        android:inputType="textPersonName" />

                                    <EditText
                                        android:id="@+id/txt_longitud"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:ems="10"
                                        android:hint="Longitud"
                                        android:inputType="textPersonName" />
                                </LinearLayout>

                                <ImageButton
                                    android:id="@+id/btn_capturar_gps"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:background="@android:drawable/btn_default_small"
                                    android:tint="@android:color/holo_green_dark"
                                    app:srcCompat="@android:drawable/ic_menu_mylocation" />

                            </LinearLayout>
                        </LinearLayout>

                    </com.google.android.material.card.MaterialCardView>
                    <!--Fin ubicacion-->

                    <!--Transformador-->
                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        app:cardBackgroundColor="@color/backgroundApp"
                        app:cardCornerRadius="5dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="5dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView69"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="10dp"
                                android:text="Transformador Asociado"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textView70"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Transformador No" />

                            <TextView
                                android:id="@+id/txt_transformador_no"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textView71"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Tipo" />

                            <TextView
                                android:id="@+id/txt_transformador_tipo"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textView20"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Potencia(KVA)" />

                            <TextView
                                android:id="@+id/txt_potencia_transformador"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textView81"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/titulo_direccion" />

                            <TextView
                                android:id="@+id/txt_direccion_transformador"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textView21"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Placa MT" />

                            <TextView
                                android:id="@+id/txt_mt_transformador"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textView22"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Placa CT" />

                            <TextView
                                android:id="@+id/txt_ct_transformador"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <!--<TextView
                                android:id="@+id/view_codigo_sai"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Código SAI" />

                            <TextView
                                android:id="@+id/txt_codigo_sai"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:textSize="16sp"
                                android:textStyle="bold"/>-->

                            <TextView
                                android:id="@+id/textView59"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Es Exclusivo AP ?" />

                            <TextView
                                android:id="@+id/txt_transformador_exclusivo_ap"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:textSize="16sp"
                                android:textStyle="bold" />


                            <!--<TextView
                                android:id="@+id/textView29"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Propietario" />

                            <TextView
                                android:id="@+id/txt_propiedad_transformador"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:textSize="16sp"
                                android:textStyle="bold"/>-->


                            <!--<View
                                android:id="@+id/divider9"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="?android:attr/listDivider" />-->

                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>
                    <!--fin Transformador-->

                    <!--Fotos-->
                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        app:cardBackgroundColor="@color/backgroundApp"
                        app:cardCornerRadius="5dp"
                        app:cardElevation="10dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dp"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/foto_1"
                                    android:layout_width="150dp"
                                    android:layout_height="150dp"
                                    android:layout_gravity="center"
                                    android:layout_margin="5dp"
                                    android:layout_weight="1"
                                    android:src="@drawable/icon_no_photography" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <Button
                                        android:id="@+id/btn_borrar_foto_1"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@android:drawable/btn_default_small"
                                        android:drawableLeft="@drawable/icon_delete"
                                        android:text="@string/btn_borrar_foto" />
                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dp"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/foto_2"
                                    android:layout_width="150dp"
                                    android:layout_height="150dp"
                                    android:layout_gravity="center"
                                    android:layout_margin="5dp"
                                    android:layout_weight="1"
                                    android:src="@drawable/icon_no_photography" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <Button
                                        android:id="@+id/btn_borrar_foto_2"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@android:drawable/btn_default_small"
                                        android:drawableLeft="@drawable/icon_delete"
                                        android:text="@string/btn_borrar_foto" />
                                </LinearLayout>

                            </LinearLayout>
                        </LinearLayout>

                    </com.google.android.material.card.MaterialCardView>

                    <!--observaciones-->

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        app:cardBackgroundColor="@color/backgroundApp"
                        app:cardCornerRadius="5dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="5dp"
                            android:orientation="vertical">

                            <EditText
                                android:id="@+id/txt_observacion"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:drawable/editbox_background_normal"
                                android:ems="10"
                                android:gravity="start|top"
                                android:hint="Observación"
                                android:inputType="textMultiLine"
                                android:lines="5" />
                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="75dp"
                        android:orientation="vertical" />
                </LinearLayout>
            </ScrollView>
        </LinearLayout>

    </androidx.appcompat.widget.LinearLayoutCompat>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_guardar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/fab_margin"
        android:tint="@color/colorIconText"
        app:backgroundTint="@color/colorAccentOk"
        app:srcCompat="@drawable/icon_save"
        tools:ignore="VectorDrawableCompat" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_cancelar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|left"
        android:layout_margin="@dimen/fab_margin"
        android:tint="@color/colorIconText"
        app:backgroundTint="@color/colorAccentCancel"
        app:srcCompat="@drawable/icon_undo"
        tools:ignore="VectorDrawableCompat" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>